                android:textColor="#000"/>
        </RelativeLayout>

        <RelativeLayout
            android:padding="5dp"
            android:id="@+id/linxuan"
            android:layout_below="@id/yixuan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
