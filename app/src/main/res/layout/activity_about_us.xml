<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

<RelativeLayout
    android:id="@+id/titleFANTA"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_margin="20dp"
    android:verticalScrollbarPosition="defaultPosition">

    <TextView
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TEAM FANTA"
        android:textSize="24sp"
        android:textColor="#000"
        android:textStyle="bold|italic"/>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp">

        <RelativeLayout
            android:id="@id/rels"
            android:padding="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/image"
                android:layout_width="150dp"
                android:layout_height="200dp"
                android:src="@drawable/joyaan"/>

            <TextView
                android:layout_marginLeft="20dp"
                android:layout_below="@id/image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Joyaan Bhesania"
                android:textColor="#000"/>
        </RelativeLayout>

        <RelativeLayout
            android:padding="5dp"
            android:id="@+id/asdf"
            android:layout_below="@id/rels"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/image2"
                android:layout_width="150dp"
                android:layout_height="200dp"
                android:src="@drawable/peter"/>

            <TextView
                android:layout_marginLeft="25dp"
                android:layout_below="@id/image2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Bingtian Huang"
                android:textColor="#000"/>
        </RelativeLayout>

        <RelativeLayout
            android:padding="5dp"
            android:id="@+id/relll"
            android:layout_toEndOf="@id/rels"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/image3"
                android:layout_width="150dp"
                android:layout_height="200dp"
                android:src="@drawable/tom"/>

            <TextView
                android:layout_marginLeft="38dp"
                android:layout_below="@id/image3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tom Toma"
                android:textColor="#000"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/adddd"
            android:padding="5dp"
            android:layout_below="@id/relll"
            android:layout_toEndOf="@id/asdf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/image4"
                android:layout_width="150dp"
                android:layout_height="200dp"
                android:src="@drawable/arun"/>

            <TextView
                android:layout_marginLeft="30dp"
                android:layout_below="@id/image4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Arun Sugumar"
                android:textColor="#000"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/adfg"
            android:padding="5dp"
            android:layout_below="@id/adddd"
            android:layout_toEndOf="@id/asdf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/image5"
                android:layout_width="150dp"
                android:layout_height="200dp"
                android:src="@drawable/jay"/>

            <TextView
                android:layout_marginLeft="42dp"
                android:layout_below="@id/image5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Jay Chitale"
                android:textColor="#000"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/adfgh"
            android:padding="5dp"
            android:layout_below="@id/asdf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/image6"
                android:layout_width="150dp"
                android:layout_height="200dp"
                android:src="@drawable/nima"/>

            <TextView
                android:layout_marginLeft="28dp"
                android:layout_below="@id/image6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nima Azbijari"
                android:textColor="#000"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/adfghg"
            android:padding="5dp"
            android:layout_below="@id/adfgh"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/image7"
                android:layout_width="150dp"
                android:layout_height="200dp"
                android:src="@drawable/oren"/>

            <TextView
                android:layout_marginLeft="28dp"
                android:layout_below="@id/image7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Oren Lenchner"
                android:textColor="#000"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/adfghgaa"
            android:padding="5dp"
            android:layout_below="@id/adfg"
            android:layout_toEndOf="@+id/adfghg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/image8"
                android:layout_width="150dp"
                android:layout_height="200dp"
                android:src="@drawable/linxuan"/>

            <TextView
                android:layout_marginLeft="32dp"
                android:layout_below="@id/image8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Linxuan Wang"
                android:textColor="#000"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/adfghga"
            android:padding="5dp"
            android:layout_below="@id/adfghg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/image9"
                android:layout_width="150dp"
                android:layout_height="200dp"
                android:src="@drawable/stephanie"/>

            <TextView
                android:layout_marginLeft="15dp"
                android:layout_below="@id/image9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Stephanie Hartono"
                android:textColor="#000"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/adfghgaaa"
            android:padding="5dp"
            android:layout_below="@id/adfghg"
            android:layout_toEndOf="@+id/adfghga"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/image10"
                android:layout_width="150dp"
                android:layout_height="200dp"
                android:src="@drawable/yixuan"/>

            <TextView
                android:layout_marginLeft="32dp"
                android:layout_below="@id/image10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Yixuan Wang"
                android:textColor="#000"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/adfghgaaaa"
            android:layout_marginTop="17dp"
            android:layout_below="@id/adfghga"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        </RelativeLayout>

    </RelativeLayout>


    </RelativeLayout>

    </ScrollView>

