<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AboutUs">


<RelativeLayout
    android:id="@+id/titleFANTA"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_margin="20dp"
    android:verticalScrollbarPosition="defaultPosition">

    <TextView
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TEAM FANTA"
        android:textSize="24sp"
        android:textColor="#000"
        android:textStyle="bold|italic"/>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp">

        <RelativeLayout
            android:id="@+id/joyaan"
            android:padding="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/image"
                android:layout_width="150dp"
                android:layout_height="200dp"
                android:src="@drawable/joyaan"
                android:scaleType="centerCrop"/>

            <TextView
                android:layout_centerHorizontal="true"
                android:layout_below="@id/image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Joyaan Bhesania"
                android:textColor="#000"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_alignParentEnd="true"
            android:padding="5dp"
            android:id="@+id/projman"
            android:layout_toEndOf="@id/joyaan"
            android:layout_width="150dp"
            android:layout_height="200dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAlignment="center"
                android:layout_centerInParent="true"
                android:text="Project Manager"
                android:textColor="#000"/>
        </RelativeLayout>

        <RelativeLayout
            android:padding="5dp"
            android:id="@+id/peter"
            android:layout_below="@id/joyaan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
